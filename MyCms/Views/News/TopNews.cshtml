@model IEnumerable<DataLayer.Page>

<section class="last-news-side border-radius">
    <header><i class="icons icon-star"></i> <h3> : Most Visited News </h3></header>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                
                <img class="border-radius" src="/PageImages/@item.ImageName">
                <h2> <a href="/News/@item.OageId">@item.Title</a></h2>
                <span>@item.CreateDate.ToLongDateString()</span>
            </li>

        }



    </ul>
</section>  

